
<%= turbo_frame_tag "webhook_frame" do %>
  <%= simple_form_for webhook do |f| %>
    <%= f.input :user_id, as: :hidden %>
    <%= f.input :url, as: :string %>
    <%= f.button :submit, class: "btn btn-primary m-4" %>
  <% end %>
  <%= link_to "Cancel", dashboard_path, class: "btn btn-danger" %>
<% end %>
